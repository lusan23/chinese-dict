#include<stdio.h>
#include<string.h>
#include <ctype.h>


int is_chinese(char * string) {
  int it_is;
  for (int i = 0; string[i] != '\0'; i++) {
    if (isalpha(string[i]) && sizeof(string) % 3 == 0){
      return 1;
    } else {
        return 0;
    }
  }
  if (it_is) {return 0;}
  
} 

int main(){
  printf("%d", is_chinese(""));
  
  return 0;
}
